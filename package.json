{
  "name": "neulab",
  "version": "0.1.1",
  "description": "Программа для оценки пространственной и рабочей памяти животных",
  "private": true,
  "releaseYear": "2024",
  "autors": [
    {
      "name": "Aleksei Drobkov",
      "email": "abkommem@gmail.com",
      "url": "https://alexeydrobkovstrikesback.github.io/neulab-release-0.1.0"
    },
    {
      "name": "Alim Bashirzade",
      "email": "alim.bashirzade95@mail.ru"
    }
  ],
  "homepage": "https://alexeydrobkovstrikesback.github.io/neulab-release-0.1.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rm -rf dist",
    "dev": "npm run dev",
    "deploy": "gh-pages -d build"
  },
  "build": {
    "appId": "neulabId",
    "productName": "neulab",
    "directories": {
      "output": "dist"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  },
  "extends": null,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "gh-pages": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1"
  },
  "devDependencies": {}
}
